#!/bin/sh
# $FreeBSD: tags/RELEASE_10_3_0/japanese/skkserv/files/skkserv.in 340872 2014-01-24 00:14:07Z mat $
#
# PROVIDE: skkserv
# REQUIRE: LOGIN
# KEYWORD: shutdown
#

. /etc/rc.subr

name=skkserv
rcvar=skkserv_enable
command=%%PREFIX%%/sbin/skkserv

: ${skkserv_enable:="NO"}
: ${skkserv_flags:="-a 127.0.0.1 -u skkserv"}

load_rc_config ${name}
run_rc_command $1

#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/sysutils/tuptime/files/tuptime.in 400493 2015-10-30 08:35:56Z danfe $
#
# PROVIDE: tuptime
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="tuptime"
rcvar=tuptime_enable

command="%%PREFIX%%/bin/tuptime"

start_cmd="${name}_start"
stop_cmd="${name}_stop"

tuptime_start()
{
	command_args="-x"

	$command $command_args
}

tuptime_stop()
{
	command_args="-xg"

	$command $command_args
}

load_rc_config $name
run_rc_command "$1"

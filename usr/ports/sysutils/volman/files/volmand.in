#!/bin/sh

# $FreeBSD: tags/RELEASE_10_3_0/sysutils/volman/files/volmand.in 369568 2014-09-30 02:13:58Z amdmi3 $
#
# PROVIDE: volmand
# REQUIRE: LOGIN
# KEYWORD: shutdown nojail

. /etc/rc.subr

name="volmand"
rcvar=volmand_enable

load_rc_config $name

: ${volmand_enable:="NO"}

pidfile="/var/run/volmand.pid"

command_interpreter="/bin/sh"
command="%%PREFIX%%/sbin/volmand"

run_rc_command "$1"


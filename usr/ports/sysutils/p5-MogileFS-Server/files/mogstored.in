#!/bin/sh

# $FreeBSD: tags/RELEASE_10_3_0/sysutils/p5-MogileFS-Server/files/mogstored.in 340872 2014-01-24 00:14:07Z mat $
#
# PROVIDE: mogstored
# REQUIRE: LOGIN cleanvar
# KEYWORD: shutdown

. /etc/rc.subr

name=mogstored
rcvar=mogstored_enable

load_rc_config mogstored

: ${mogstored_enable:="NO"}

required_files=%%PREFIX%%/etc/mogstored.conf

command="%%PREFIX%%/bin/${name}"
command_args="--config=%%PREFIX%%/etc/mogstored.conf -d"

run_rc_command "$1"

#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/sysutils/metalog/files/metalog.in 340872 2014-01-24 00:14:07Z mat $
#

# PROVIDE: metalog
# REQUIRE: mountcritremote cleanvar newsyslog ldconfig
# BEFORE:  SERVERS

. /etc/rc.subr

name=metalog
rcvar=metalog_enable
pidfile="/var/run/metalog.pid"
command="%%PREFIX%%/sbin/${name}"
command_args="--daemonize"
required_files="%%PREFIX%%/etc/metalog.conf"

load_rc_config $name
: ${metalog_enable:="NO"}
run_rc_command "$1"

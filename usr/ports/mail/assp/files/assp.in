#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/mail/assp/files/assp.in 340872 2014-01-24 00:14:07Z mat $
#

# PROVIDE: assp
# REQUIRE: LOGIN
# KEYWORD: shutdown

. /etc/rc.subr

name=assp
rcvar=assp_enable

command=%%PREFIX%%/sbin/assp
command_interpreter=%%PERL%%
pidfile=%%ASSP_HOME%%/pid

load_rc_config $name
: ${assp_enable="NO"}
: ${assp_flags="%%ASSP_HOME%%"}

run_rc_command "$1"

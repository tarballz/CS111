#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/www/obhttpd/files/obhttpd.in 403107 2015-12-06 12:17:51Z riggs $
#
# PROVIDE: obhttpd
# REQUIRE: DAEMON
# BEFORE:  LOGIN
# KEYWORD: shutdown

# Add the following lines to /etc/rc.conf to enable obhttpd:
# obhttpd_enable="YES"
# obhttpd_flags="<set as needed>"

. /etc/rc.subr

name=obhttpd
rcvar=obhttpd_enable

command="%%PREFIX%%/sbin/obhttpd"

obhttpd_enable=${obhttpd_enable:-"NO"}
obhttpd_flags=${obhttpd_flags:-"-f %%PREFIX%%/etc/obhttpd.conf"}

load_rc_config $name
run_rc_command "$1"

#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/www/hydra/files/pkg-deinstall.in 340719 2014-01-22 15:52:06Z mat $
#

case $2 in
	DEINSTALL)
		;;
	POST-DEINSTALL)
		if [ ! -d %%LOGDIR%% ] ; then
			rmdir %%LOGDIR%%
		else
			echo "--------------------------------------------"
			echo " Don't forget to delete %%LOGDIR%% when"
			echo " you've finished with hydra's log files."
			echo "--------------------------------------------"
		fi
		;;
esac

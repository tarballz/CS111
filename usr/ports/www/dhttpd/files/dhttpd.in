#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/www/dhttpd/files/dhttpd.in 340872 2014-01-24 00:14:07Z mat $
#
# PROVIDE: dhttpd
# REQUIRE: DAEMON
# KEYWORD: shutdown
#
# Add the following line to /etc/rc.conf to enable dhttpd:
#
# dhttpd_enable (bool):	Set it to "YES" to enable dhttpd
#

. /etc/rc.subr

name="dhttpd"
rcvar=dhttpd_enable

load_rc_config $name

: ${dhttpd_enable="NO"}

command=%%PREFIX%%/sbin/${name}

run_rc_command "$1"

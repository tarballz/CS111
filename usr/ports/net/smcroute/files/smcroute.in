#!/bin/sh
# $FreeBSD: tags/RELEASE_10_3_0/net/smcroute/files/smcroute.in 396339 2015-09-07 19:00:57Z hrs $
#
# PROVIDE: smcroute
# REQUIRE: netif routing
# KEYWORD: nojail

#
# Add the following to /etc/rc.conf[.local] to enable this service
#
# smcroute_enable="YES"
#

. /etc/rc.subr

name="smcroute"
rcvar="${name}_enable"
command="%%PREFIX%%/sbin/${name}"
start_precmd="${name}_precmd"

smcroute_precmd()
{

	rc_flags="$rc_flags -d"
}

load_rc_config $name

: ${smcroute_enable="NO"}
: ${smcroute_flags=""}

run_rc_command "$1"

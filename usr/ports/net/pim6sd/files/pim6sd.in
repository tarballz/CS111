#!/bin/sh
# $FreeBSD: tags/RELEASE_10_3_0/net/pim6sd/files/pim6sd.in 396342 2015-09-07 19:23:26Z hrs $
#
# PROVIDE: pim6sd
# REQUIRE: netif routing
# KEYWORD: nojail

#
# Add the following to /etc/rc.conf[.local] to enable this service
#
# pim6sd_enable="YES"
#

. /etc/rc.subr

name="pim6sd"
rcvar="${name}_enable"
command="%%PREFIX%%/sbin/${name}"
required_files="%%PREFIX%%/etc/${name}.conf"
extra_commands=reload

load_rc_config $name
: ${pim6sd_enable="NO"}
run_rc_command "$1"


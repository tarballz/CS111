#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/net/ntop/files/pkg-deinstall.in 340719 2014-01-22 15:52:06Z mat $
#

case $2 in
	DEINSTALL)
		;;
	POST-DEINSTALL)
		if [ -d %%DBDIR%%/ntop ];then
		echo "===> post-deinstallation information for $1"
		echo ""
		echo "     Please note that ntop was not completely removed"
		echo "     from this system:"
		echo ""
		echo "     %%DBDIR%%/ntop can be removed if this port will not be"
		echo "     reinstalled."
		echo ""
		fi
		;;
	*)
		exit 64
		;;
esac
exit 0

#!/bin/sh
# $FreeBSD: tags/RELEASE_10_3_0/net/dictd/files/dictd.in 374253 2014-12-08 09:12:03Z sunpoet $
#
# PROVIDE: dictd
# REQUIRE: DAEMON
#
# Add the following line to /etc/rc.conf[.local] to enable dictd:
#
# dictd_enable="YES"
#

dictd_enable=${dictd_enable:-"NO"}

. /etc/rc.subr

name=dictd
rcvar=dictd_enable
command=%%PREFIX%%/sbin/dictd

load_rc_config $name
run_rc_command "$1"

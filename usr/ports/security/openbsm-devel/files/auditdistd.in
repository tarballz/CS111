#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/security/openbsm-devel/files/auditdistd.in 312271 2013-02-15 03:05:58Z zi $
#

# PROVIDE: auditdistd
# REQUIRE: auditd
# BEFORE:  DAEMON
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="auditdistd"
rcvar="${name}_enable"
pidfile="/var/run/${name}.pid"
command="%%PREFIX%%/sbin/${name}"
required_files="/etc/security/${name}.conf"
extra_commands="reload"

load_rc_config $name
run_rc_command "$1"

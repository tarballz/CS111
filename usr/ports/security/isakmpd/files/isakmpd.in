#!/bin/sh
#
# $FreeBSD: tags/RELEASE_10_3_0/security/isakmpd/files/isakmpd.in 340872 2014-01-24 00:14:07Z mat $
#

# PROVIDE: ike isakmpd
# REQUIRE: root mountcritlocal
# BEFORE:  DAEMON
# KEYWORD: nojail

. /etc/rc.subr

name="isakmpd"
rcvar=isakmpd_enable
command="%%PREFIX%%/sbin/${name}"
extra_commands="reload"

load_rc_config $name

: ${isakmpd_enable="NO"}

run_rc_command "$1"
